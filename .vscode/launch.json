    "configurations": [
        {
            "name"   : "Launch",
            "type"   : "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cmake-build-release/hydra_app/hydra",
            "args"   : ["-nowindow", "1", 
                        "-saveinterval", "600",
                        "-enable_mlt", "0",
                        "-out",    "/home/frol/temp/test_out/image.png",
                        "-outall", "/home/frol/temp/test_out/gbuff",
                        "-inputlib", "/home/frol/PROG/HydraAPI/main/tests/test_77",
                        "-evalgbuffer", "1"],

            "stopAtEntry": false,
            "cwd"    : "${workspaceFolder}/hydra_app",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cmake-build-debug/hydra_app/hydra",
            "args": ["-nowindow", "1",
                     "-saveinterval", "300",
                     "-enable_mlt", "0"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/hydra_app",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
        
        

